        "status": {
            "title": "Job Status",
            "type": "string",
            "description": "Select job status or leave blank for any.",
            "enum": ["", "open", "closed"],
            "enumTitles": ["Any status", "Open", "Closed"],
            "editor": "select",
            "default": ""
        },
        "jobType": {
            "title": "Job Type",
            "type": "string",
            "description": "Contract type for the engagement.",
            "enum": ["", "hourly", "fixed-price"],
            "enumTitles": ["Any type", "Hourly", "Fixed price"],
            "editor": "select",
            "default": ""
