        "maxJobs": {
            "title": "Maximum Jobs",
            "type": "integer",
            "description": "Number of jobs to fetch per run (1-50)",
            "minimum": 1,
            "maximum": 50,
            "default": 20,
            "editor": "number"
        },
        "offset": {
            "title": "Pagination Offset",
            "type": "integer",
            "description": "Skip this many matching jobs before returning results. Must be a multiple of the limit (maxJobs).",
