{
    "actorSpecification": 1,
    "fields": {
        "title": "Upwork Job Listings",
        "type": "object",
        "schemaVersion": 1,
        "properties": {
        "job_id": {
            "title": "Job ID",
            "type": "string",
            "description": "Unique identifier for the job posting",
            "example": "Virtual-Assistant-Needed-for-Research-Outreach-and-Organization_~021971791858643452240"
        },
        "title": {
            "title": "Job Title",
            "type": "string",
            "description": "The title of the job posting",
            "example": "Virtual Assistant Needed for Research, Outreach, and Organization"
        },
        "description": {
            "title": "Job Description", 
            "type": "string",
            "description": "Full description of the job requirements and details",
            "example": "We are seeking an experienced virtual assistant to help with research, outreach, and organization tasks..."
        },
        "url": {
            "title": "Job URL",
            "type": "string",
            "description": "Direct link to the job posting on Upwork",
            "example": "https://www.upwork.com/jobs/Virtual-Assistant-Needed-for-Research-Outreach-and-Organization_~021971791858643452240/",
            "format": "uri"
        },
        "hourly_rate": {
            "title": "Hourly Rate",
            "type": "string",
            "description": "Hourly rate range for the job",
            "example": "$25.00 - $47.00/hr"
        },
        "budget": {
            "title": "Project Budget",
            "type": "string", 
            "description": "Fixed price budget for the project",
            "example": "$400.00"
        },
        "experience_level": {
            "title": "Experience Level",
            "type": "string",
            "description": "Required experience level for the job",
            "example": "Intermediate"
        },
        "job_type": {
            "title": "Job Type",
            "type": "string", 
            "description": "Type of job posting",
            "example": "Fixed-price"
        },
        "skills": {
            "title": "Required Skills",
            "type": "array",
            "description": "List of skills required for the job",
            "items": {
                "type": "string"
            },
            "example": ["Web Development", "Web Design", "PHP"]
        },
        "client_location": {
            "title": "Client Location",
            "type": "string",
            "description": "Geographic location of the client",
            "example": "United States"
        },
        "client_company_size": {
            "title": "Client Company Size",
            "type": "string",
            "description": "Size of the client's company",
            "example": "Mid-sized company (10-99 people)"
        },
        "client_industry": {
            "title": "Client Industry",
            "type": "string",
            "description": "Industry sector of the client",
            "example": "Health & Fitness"
        },
        "posted_date": {
            "title": "Posted Date",
            "type": "string",
            "description": "When the job was posted (relative time)",
            "example": "Posted 11 minutes ago"
        },
        "proposals_count": {
            "title": "Number of Proposals",
            "type": "string",
            "description": "Number of proposals submitted for this job",
            "example": "Less than 5"
        },
        "duration": {
            "title": "Project Duration",
            "type": "string",
            "description": "Expected duration of the project",
            "example": "1-3 months"
        },
        "project_type": {
            "title": "Project Type",
            "type": "string",
            "description": "Type of project engagement",
            "example": "One-time project"
        },
        "work_hours": {
            "title": "Work Hours",
            "type": "string",
            "description": "Expected weekly work hours",
            "example": "Less than 30 hrs/week"
        },
        "member_since": {
            "title": "Client Member Since",
            "type": "string",
            "description": "When the client joined Upwork",
            "example": "Oct 20, 2015"
        },
        "total_spent": {
            "title": "Total Client Spending",
            "type": "string",
            "description": "Total amount the client has spent on Upwork",
            "example": "$23K"
        },
        "total_hires": {
            "title": "Total Hires",
            "type": "string",
            "description": "Total number of freelancers hired by the client",
            "example": "59"
        },
        "last_visited_at": {
            "title": "Last Visited",
            "type": "string",
            "description": "When this job was last checked by the scraper",
            "format": "date-time"
        },
        "scraped_at": {
            "title": "Scraped At",
            "type": "string", 
            "description": "When this data was extracted",
            "format": "date-time"
        },
        "raw_data": {
            "title": "Raw Data",
            "type": "object",
            "description": "Complete raw data from the API response (when include_raw_data is enabled)"
        }
    },
    "required": ["job_id", "title", "url", "scraped_at"]
    },
    "views": {
        "overview": {
            "title": "Job Overview",
            "transformation": {
                "fields": [
                    "title",
                    "experience_level", 
                    "job_type",
                    "client_location",
                    "proposals_count",
                    "posted_date",
                    "budget",
                    "hourly_rate"
                ]
            },
            "display": {
                "component": "table",
                "properties": {
                    "title": {
                        "label": "Job Title",
                        "format": "text"
                    },
                    "experience_level": {
                        "label": "Experience",
                        "format": "tag"
                    },
                    "job_type": {
                        "label": "Type", 
                        "format": "tag"
                    },
                    "client_location": {
                        "label": "Location",
                        "format": "text"
                    },
                    "proposals_count": {
                        "label": "Proposals",
                        "format": "text"
                    },
                    "posted_date": {
                        "label": "Posted",
                        "format": "text"
                    },
                    "budget": {
                        "label": "Budget",
                        "format": "text"
                    },
                    "hourly_rate": {
                        "label": "Hourly Rate",
                        "format": "text"
                    }
                }
            }
        },
        "detailed": {
            "title": "Detailed View",
            "transformation": {
                "fields": [
                    "title",
                    "description", 
                    "hourly_rate",
                    "budget",
                    "skills",
                    "experience_level",
                    "job_type",
                    "client_location",
                    "client_company_size",
                    "client_industry",
                    "proposals_count",
                    "posted_date",
                    "duration",
                    "project_type",
                    "work_hours",
                    "total_spent",
                    "total_hires",
                    "url"
                ]
            },
            "display": {
                "component": "table",
                "properties": {
                    "title": {
                        "label": "Title",
                        "format": "text"
                    },
                    "description": {
                        "label": "Description", 
                        "format": "text",
                        "truncate": 150
                    },
                    "hourly_rate": {
                        "label": "Hourly Rate",
                        "format": "text"
                    },
                    "budget": {
                        "label": "Budget",
                        "format": "text" 
                    },
                    "skills": {
                        "label": "Skills",
                        "format": "array"
                    },
                    "client_company_size": {
                        "label": "Company Size",
                        "format": "text"
                    },
                    "client_industry": {
                        "label": "Industry",
                        "format": "text"
                    },
                    "total_spent": {
                        "label": "Client Spent",
                        "format": "text"
                    },
                    "total_hires": {
                        "label": "Total Hires",
                        "format": "text"
                    },
                    "url": {
                        "label": "Job Link",
                        "format": "link"
                    }
                }
            }
        }
    }
}
