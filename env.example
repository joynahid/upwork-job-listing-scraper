# Apify Configuration
# Get your token from https://console.apify.com/account/integrations
APIFY_TOKEN=your_apify_token_here

# Apify Proxy Configuration (if using proxy)
APIFY_PROXY_GROUP=RESIDENTIAL
APIFY_PROXY_PASSWORD=your_proxy_password_here

# Logging Configuration
APIFY_LOG_LEVEL=INFO

# Local Storage Directory (for development)
APIFY_LOCAL_STORAGE_DIR=./storage

# Browser Configuration
# Set to 'true' to run browsers in headless mode
HEADLESS=true

# Development Configuration
DEVELOPMENT=false
DEBUG_MODE=false

# Optional: Custom User Agent
# USER_AGENT="Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"

# Optional: Request Timeout (in seconds)
# REQUEST_TIMEOUT=30

# Optional: Maximum concurrent requests
# MAX_CONCURRENCY=5

# Optional: Request delay (in milliseconds)
# REQUEST_DELAY=1000
