# HTTP Proxy Configuration
# Full HTTP proxy URL (e.g., http://username:password@proxy.example.com:8000)
PROXY_URL=

# Logging Configuration
LOG_LEVEL=INFO

# Local Storage Directory (for development)
LOCAL_STORAGE_DIR=./storage

# Browser Configuration
# Set to 'true' to run browsers in headless mode
HEADLESS=true

# Development Configuration
DEVELOPMENT=false
DEBUG_MODE=false

# Optional: Custom User Agent
# USER_AGENT="Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36"

# Optional: Request Timeout (in seconds)
# REQUEST_TIMEOUT=30

# Optional: Maximum concurrent requests
# MAX_CONCURRENCY=5

# Optional: Request delay (in milliseconds)
# REQUEST_DELAY=1000

# SQLite Database Configuration
# Directory path for SQLite persistent storage (keeping ROCKSDB_DATA_DIR name for compatibility)
ROCKSDB_DATA_DIR=./sqlite_data
