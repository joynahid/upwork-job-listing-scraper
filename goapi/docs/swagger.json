        "/job-list": {
            "get": {
                "security": [
                    {
                        "ApiKeyAuth": []
                    }
                ],
                "description": "Retrieve Upwork job summaries from the job_list collection with optional filters.",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "job-list"
                ],
                "summary": "List job summaries",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "Number of records (1-50)",
                        "name": "limit",
                        "in": "query"
                    },
                    {
                        "type": "boolean",
                        "description": "Filter by client payment verification",
                        "name": "payment_verified",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Filter by client country",
                        "name": "country",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Comma-separated list of required skill labels",
                        "name": "skills",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Filter by job type (hourly|fixed-price or numeric code)",
                        "name": "job_type",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Filter by duration label",
                        "name": "duration",
                        "in": "query"
                    },
                    {
                        "type": "number",
                        "description": "Minimum hourly budget",
                        "name": "hourly_min",
                        "in": "query"
                    },
                    {
                        "type": "number",
                        "description": "Maximum hourly budget",
                        "name": "hourly_max",
                        "in": "query"
                    },
                    {
                        "type": "number",
                        "description": "Minimum fixed budget",
                        "name": "budget_min",
                        "in": "query"
                    },
                    {
                        "type": "number",
                        "description": "Maximum fixed budget",
                        "name": "budget_max",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Case-insensitive search term for title or description",
                        "name": "search",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Sort mode (published_on_asc, published_on_desc, last_visited_asc, last_visited_desc)",
                        "name": "sort",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/server.JobListResponse"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/server.JobListResponse"
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "$ref": "#/definitions/server.JobListResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/server.JobListResponse"
                        }
                    }
                }
            }
        },
        "/jobs": {
            "get": {
                "security": [
                    {
                        "ApiKeyAuth": []
                    }
                ],
                "description": "Retrieve normalized job documents with optional filters.",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "jobs"
                ],
                "summary": "List jobs",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "Number of records (1-50)",
                        "name": "limit",
                        "in": "query"
                    },
                    {
                        "type": "boolean",
                        "description": "Filter by payment verification",
                        "name": "payment_verified",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Filter by category slug",
                        "name": "category",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Filter by category group slug",
                        "name": "category_group",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Filter by job status (open|closed or numeric code)",
                        "name": "status",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Filter by job type (hourly|fixed-price or numeric code)",
                        "name": "job_type",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Filter by contractor tier (entry|intermediate|expert or numeric code)",
                        "name": "contractor_tier",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Filter by buyer country",
                        "name": "country",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Comma-separated required tags",
                        "name": "tags",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "ISO timestamp lower bound",
                        "name": "posted_after",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "ISO timestamp upper bound",
                        "name": "posted_before",
                        "in": "query"
                    },
                    {
                        "type": "number",
                        "description": "Minimum fixed budget",
                        "name": "budget_min",
                        "in": "query"
                    },
                    {
                        "type": "number",
                        "description": "Maximum fixed budget",
                        "name": "budget_max",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Sort mode (posted_on_asc, posted_on_desc, last_visited_asc, last_visited_desc)",
                        "name": "sort",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/server.JobsResponse"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/server.JobsResponse"
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "$ref": "#/definitions/server.JobsResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/server.JobsResponse"
                        }
                    }
                }
            }
        }
    },
